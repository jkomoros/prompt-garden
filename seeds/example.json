{
    "version": 0,
    "seeds": {
        "": {
            "type": "prompt",
            "description": "This is the default seed",
            "prompt": "Create a limerick about gardens."
        },
        "hello-world": {
            "type": "log",
            "value": "Hello, world!"
        },
        "input-example": {
            "type": "input",
            "question": "What is your name?",
            "default": "Bob"
        },
        "composed-prompt": {
            "type": "prompt",
            "description": "This seed demonstrates a seed that references sub seeds",
            "prompt": {
                "id": "hello-world"
            }
        },
        "input-prompt": {
            "type": "prompt",
            "prompt": {
                "type": "template",
                "template": "Write a limerick about a person named {{name}}.",
                "vars": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "input",
                            "question": "What is the name to use?",
                            "default": "Bob"
                        }
                    }
                }
            }
        },
        "template-example": {
            "type": "template",
            "template": "{{name}} is {{age}}",
            "vars": {
                "name": "Bob",
                "age": 5,
                "unusedVar": "This var is unused"
            }
        },
        "remote-example": {
            "type": "log",
            "value": {
                "packet": "https://raw.githubusercontent.com/jkomoros/prompt-garden/main/seeds/default.json",
                "id": "hello-world"
            }
        },
        "nested-example": {
            "type": "log",
            "value": {
                "id": "nested",
                "type": "log",
                "value": 3
            }
        }
    }
}