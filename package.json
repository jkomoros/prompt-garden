{
    "name": "prompt-garden",
    "type": "module",
    "scripts": {
        "build": "npm run build:typescript && npm run build:rollup",
        "build:typescript": "tsc",
        "build:rollup": "rollup -c",
        "serve": "tsc --watch & wds --node-resolve --port=8081",
        "test": "npm run test:base && npm run test:template",
        "test:base": "mocha -r esm test/base",
        "test:template": "mocha -r esm test/template",
        "generate:schema": "npx ts-node-esm tools/schema/main.ts"
    },
    "bin": {
        "garden": "./tools/garden/main.js"
    },
    "dependencies": {
        "eslint": "^8.43.0",
        "esm-seedrandom": "^3.0.5",
        "gpt-tok": "^0.0.1",
        "hnswlib-node": "^2.0.0",
        "inquirer": "^9.2.7",
        "lit": "^2.8.0",
        "openai": "^3.3.0",
        "pwa-helpers": "^0.9.1",
        "redux": "^4.2.1",
        "redux-thunk": "^2.4.2",
        "reselect": "^4.1.8",
        "ts-command-line-args": "^2.5.1",
        "typescript": "^5.1.3",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/inquirer": "^9.0.3",
        "@types/mocha": "^10.0.1",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@web/dev-server": "^0.3.0",
        "@web/dev-server-rollup": "^0.5.2",
        "esm": "^3.2.25",
        "mocha": "^10.2.0",
        "ts-node": "^10.9.1",
        "zod-to-json-schema": "^3.21.2"
    }
}
