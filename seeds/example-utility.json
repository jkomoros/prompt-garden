{
    "version": 0,
    "seeds": {
        "expect": {
            "description": "Useful for a meta-node to check at the top that required parameters are set.",
            "type": "map",
            "items": {
                "type": "var",
                "name": "arg:args"
            },
            "block": {
                "type": "var",
                "name": {
                    "type": "var",
                    "name": "value"
                },
                "else": {
                    "type": "throw",
                    "error": {
                        "type": "render",
                        "template": "Argument {{key}} is expected but was not set",
                        "vars": {
                            "key": {
                                "type": "var",
                                "name": "value"
                            }
                        }
                    }
                }
            }
        },
        "remove-numbers": {
            "type": "function",
            "arguments": [
                "arg:input"
            ],
            "block": {
                "type": "join",
                "delimiter": "\n",
                "items": {
                    "type": "map",
                    "items": {
                        "type": "split",
                        "input": {
                            "type": "var",
                            "name": "arg:input"
                        },
                        "delimiter": "\n"
                    },
                    "block": {
                        "type": "call",
                        "arguments": {
                            "arg:input": {
                                "type": "var",
                                "name": "value"
                            }
                        },
                        "function": {
                            "seed": "remove-item-number"
                        }
                    }
                }
            }
        },
        "remove-item-number": {
            "description": "Removes a possible 1) or 1. from the front of a line",
            "type": "function",
            "arguments": [
                "arg:input"
            ],
            "block": {
                "type": "property",
                "object": {
                    "type": "extract",
                    "template": "{{index|int|optional}}{{separator|choice:'. '|choice:') '|optional}}{{content}}",
                    "input": {
                        "type": "var",
                        "name": "arg:input"
                    }
                },
                "property": "content"
            }
        },
        "memorize-items": {
            "type": "function",
            "arguments": [
                "arg:items",
                "arg:memory"
            ],
            "block": {
                "type": "let",
                "name": "length",
                "value": {
                    "type": "property",
                    "object": {
                        "type": "var",
                        "name": "arg:items"
                    },
                    "property": "length"
                },
                "block": {
                    "type": "map",
                    "items": {
                        "type": "var",
                        "name": "arg:items"
                    },
                    "block": {
                        "type": "array",
                        "return": "last",
                        "items": [
                            {
                                "type": "log",
                                "value": {
                                    "type": "render",
                                    "template": "Memorizing item {{i}}/{{length}}",
                                    "vars": {
                                        "i": {
                                            "type": "var",
                                            "name": "key"
                                        },
                                        "length": {
                                            "type": "var",
                                            "name": "length"
                                        }
                                    }
                                }
                            },
                            {
                                "type": "memorize",
                                "memory": {
                                    "type": "var",
                                    "name": "arg:memory"
                                },
                                "value": {
                                    "type": "var",
                                    "name": "value"
                                }
                            }
                        ]
                    }
                }
            }
        }
    }
}