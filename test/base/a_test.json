{
    "version": 0,
    "seeds": {
        "": {
            "type": "prompt",
            "prompt": "Create a limerick about gardens."
        },
        "hello-world": {
            "type": "log",
            "value": "Hello, world!"
        },
        "composed-prompt": {
            "type": "prompt",
            "description": "Tests that a reference with a # works",
            "prompt": {
                "id": "hello-world"
            }
        },
        "true": {
            "type": "log",
            "value": true
        },
        "false": {
            "type": "log",
            "value": false
        },
        "if-true": {
            "type": "if",
            "test": {
                "id": "true"
            },
            "then": {
                "id": "true"
            },
            "else": {
                "id": "false"
            }
        },
        "if-false": {
            "type": "if",
            "test": {
                "id": "false"
            },
            "then": {
                "id": "true"
            },
            "else": {
                "id": "false"
            }
        },
        "render-test": {
            "type": "render",
            "template": "{{name}} is {{age}}",
            "vars": {
                "name": "Bob",
                "age": "5"
            }
        },
        "extract-test": {
            "type": "extract",
            "template": "{{name}} is {{age}}",
            "input": "Alex is 5"
        },
        "non-computed-object": {
            "type": "object",
            "properties": {
                "a": 5,
                "b": true
            }
        },
        "computed-object": {
            "type": "object",
            "properties": {
                "a": {
                    "type": "log",
                    "value": 5
                },
                "b": true
            }
        }
    }
}