{
    "version": 0,
    "seeds": {
        "": {
            "type": "prompt",
            "prompt": "Create a limerick about gardens."
        },
        "hello-world": {
            "type": "log",
            "value": "Hello, world!"
        },
        "composed-prompt": {
            "type": "prompt",
            "description": "Tests that a reference with a # works",
            "prompt": {
                "seed": "hello-world"
            }
        },
        "true": {
            "type": "log",
            "value": true
        },
        "false": {
            "type": "log",
            "value": false
        },
        "if-true": {
            "type": "if",
            "test": {
                "seed": "true"
            },
            "then": {
                "seed": "true"
            },
            "else": {
                "seed": "false"
            }
        },
        "if-false": {
            "type": "if",
            "test": {
                "seed": "false"
            },
            "then": {
                "seed": "true"
            },
            "else": {
                "seed": "false"
            }
        },
        "render-test": {
            "type": "render",
            "template": "{{name}} is {{age}}",
            "vars": {
                "name": "Bob",
                "age": "5"
            }
        },
        "extract-test": {
            "type": "extract",
            "template": "{{name}} is {{age}}",
            "input": "Alex is 5"
        },
        "non-computed-object": {
            "type": "object",
            "properties": {
                "a": 5,
                "b": true
            }
        },
        "computed-object": {
            "type": "object",
            "properties": {
                "a": {
                    "type": "log",
                    "value": 5
                },
                "b": true
            }
        },
        "computed-array": {
            "type": "array",
            "items": [
                3,
                {
                    "type": "log",
                    "value": 5
                }
            ]
        },
        "let-test": {
            "type": "let",
            "name": "foo",
            "value": {
                "type": "log",
                "value": 3
            },
            "block": {
                "type": "render",
                "template": "{{age}} is great",
                "vars": {
                    "type": "object",
                    "properties": {
                        "age": {
                            "type": "var",
                            "name": "foo"
                        }
                    }
                }
            }
        },
        "let-multi-test": {
            "type": "let-multi",
            "values": {
                "type": "object",
                    "properties": {
                    "name": "Alex",
                    "foo": {
                        "type": "log",
                        "value": 3
                    }
                }
            },
            "block": {
                "type": "render",
                "template": "{{age}} is {{name}}",
                "vars": {
                    "type": "object",
                    "properties": {
                        "age": {
                            "type": "var",
                            "name": "foo"
                        },
                        "name": {
                            "type": "var",
                            "name": "name"
                        }
                    }
                }
            }
        },
        "let-test-secret-key": {
            "type": "let",
            "name": "openai_secret_key",
            "value": 5,
            "block": 5
        },
        "embed-test": {
            "type": "embed",
            "text": "This is text to embed"
        },
        "memorize-test": {
            "type": "memorize",
            "value": "Apple"
        },
        "memorize-multiple-test": {
            "type": "memorize",
            "value": [
                "Apple",
                "Carrot",
                "Cake",
                "Car"
            ]
        },
        "recall-test": {
            "type": "recall",
            "query": "Broccoli",
            "k": 5
        },
        "recall-test-no-k": {
            "type": "recall",
            "query": "Broccoli"
        },
        "embedding-as-string": {
            "type": "render",
            "template": {
                "type": "embed",
                "text": "This is an embedding"
            },
            "vars": {}
        },
        "token-count-test": {
            "type": "token_count",
            "text": "abcdefgh"
        },
        "token-count-multiple-test": {
            "type": "token_count",
            "text": [
                "abcd",
                "abcdefgh"
            ]
        },
        "store-test": {
            "type": "store",
            "name": "foo",
            "value": 3
        },
        "retrieve-test": {
            "type": "retrieve",
            "name": "foo"
        },
        "delete-test": {
            "type": "delete",
            "name": "foo"
        },
        "private": {
            "type": "noop",
            "private": true,
            "value": true
        },
        "reference-test": {
            "type": "reference",
            "seed_id": "private"
        }
    }
}